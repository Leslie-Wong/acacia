{
  "name": "savannabits/acacia-generator",
  "description": "Savannabits Acacia's Module Generation Engine, inspired by nwidart/laravel-modules",
  "keywords": [
    "modules",
    "laravel",
    "savannabits",
    "module",
    "rad"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Samson Maosa",
      "email": "maosa.sam@gmail.com",
      "homepage": "https://savannabits.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mockery/mockery": "~1.0",
    "orchestra/testbench": "^6.2",
    "friendsofphp/php-cs-fixer": "^2.16",
    "laravel/framework": "^v8.83|^v9.2.0",
    "spatie/phpunit-snapshot-assertions": "^4.2.10",
    "phpstan/phpstan": "^1.4.6"
  },
  "autoload": {
    "psr-4": {
      "Savannabits\\AcaciaGenerator\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Savannabits\\AcaciaGenerator\\Tests\\": "tests",
      "Modules\\Recipe\\": "tests/stubs/valid/Recipe"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Savannabits\\AcaciaGenerator\\AcaciaGeneratorServiceProvider"
      ],
      "aliases": {
        "Module": "Savannabits\\AcaciaGenerator\\Facades\\Module"
      }
    },
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "scripts": {
    "update-snapshots": "./vendor/bin/phpunit --no-coverage -d --update-snapshots",
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --debug --coverage-html coverage",
    "pcf": "vendor/bin/php-cs-fixer fix --verbose"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
